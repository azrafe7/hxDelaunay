	<set name="SHOW_CONSOLE" value="1" if="debug" />

	<haxelib name="openfl" />
	<assets path="src" include="*.png" />

	<!-- advanced telemetry for use with Scout -->
	<!--<haxeflag name="-D advanced-telemetry" if="flash" />-->
</project>